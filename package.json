{
  "name": "mindbody-mcp",
  "version": "1.0.0",
  "description": "Mindbody Data Migration & Management MCP Server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "inspect": "bun run src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:connectivity": "bun run src/test-connectivity.ts"
  },
  "keywords": [
    "mcp",
    "mindbody",
    "migration",
    "api"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0"
  },
  "engines": {
    "bun": ">=1.1.0"
  }
}
